{
  "name": "rdash-angular2",
  "version": "0.0.2",
  "description": "AngularJS2 implementation of the RDash admin dashboard theme http://rdash.github.io",
  "repository": "https://github.com/ziyasal/rdash-angular2",
  "scripts": {
    "postinstall": "npm run typings",
    "tsc": "tsc",
    "tsc:w": "tsc --watch",
    "live": "live-server --open=src",
    "start": "concurrently \"npm run tsc:w\" \"npm run live\" ",
    "typings": "typings install"
  },
  "author": "Ziya SARIKAYA <ziyasal>",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^3.5.1",
    "live-server": "^1.2.0",
    "typescript": "^2.6.2",
    "typings": "^2.1.1"
  },
  "dependencies": {
    "angular2": "2.0.0-beta.21",
    "bootstrap": "^4.0.0",
    "es6-shim": "^0.35.0",
    "font-awesome": "^4.6.1",
    "jquery": "^3.3.0",
    "rdash-ui": "^1.0.1",
    "reflect-metadata": "0.1.12",
    "rxjs": "5.5.6",
    "systemjs": "0.20.19",
    "zone.js": "0.8.20"
  }
}
